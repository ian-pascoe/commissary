{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "http",
  "description": "enables http permissions",
  "windows": ["main"],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:8080"
        },
        {
          "url": "https://*.commissary.dev"
        }
      ]
    }
  ]
}
